{
    "manifest_version": 3,

    "name": "YouTube AD Blocker",
    "description": "Block ads on YouTube",
    "version": "1.3",

    "permissions": [
        "activeTab"
    ],

    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["script.js"]
        }
    ],

    "icons": {
        "48": "icon.png",
        "96": "icon.png"
    }
}
